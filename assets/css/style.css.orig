* { margin:0px; padding:0px; }
body { margin:0px; padding:0px; font-family: 'Poppins', sans-serif; background:url(../images/landing-bg.jpg) left top no-repeat; background-size:cover;position:relative; }
a,button {-webkit-transition: all ease 0.7s;-moz-transition: all ease 0.7s;-o-transition: all ease 0.7s;transition: all ease 0.7s; outline:none;}
img { outline:none; border:0px;max-width:100%; }
.clear { clear:both;}
.wrapper { height:100%; }
.container { width:100%; max-width:1405px; margin:0 auto;padding:0; }
a { text-decoration: none;color:#3d9fd9;  }
/*components*/
.list-inline { list-style: none;  }
.list-inline li { display: inline-block; }
.tabs-inline .nav-tabs li { padding-right: 30px;margin-bottom: 20px;  } 
.tabs-inline .nav-tabs li a { font-size: 18px;color: #4d4d4d;font-weight: 300;padding:0 12px 11px 12px; }
.tabs-inline .nav-tabs li.ui-state-active a { color:#3d9fd9;border-bottom:2px solid #3d9fd9;  }
.tabs-inline .ui-tabs-panel { margin-top: 21px; }



/*Sidebar*/
.blue-bg-cont {  height:100%;  }
.blue-bg-cont::after { display:block; content:''; clear:both; }
.left-side-bar { width:275px;  height:380px; background:rgba(61, 159, 217, 0.36) ;position:fixed; top:0; bottom:0; }
.left-side-bar .logo { padding:15px 0 13px 24px; background:#092639; }
