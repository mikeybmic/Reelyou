.right-cont-box { background:#f6f6f6; margin-left:275px; padding-bottom:220px;z-index:10;position: relative; transition: all .25s linear;-webkit-transition: all .25s linear;-moz-transition: all .25s linear;-o-transition: all .25s linear;    min-height: calc(100vh - 222px);min-height: -webkit-calc(100vh - 222px);min-height: -moz-calc(100vh - 222px);min-height: -o-calc(100vh - 222px); }
