.left-side-bar .user-info .img { float:left; width:50px; height:50px; border-radius:50%; }
.left-side-bar .user-info .name { float:left; color:#f7f7f7; padding:12px 0 0 8px;  font-weight:300; font-size:18px; }
.left-side-bar nav { font-size:18px; font-weight:300; }
.left-side-bar nav ul { list-style:none; }
.left-side-bar nav ul li { display:block; }
.left-side-bar nav ul li a { display:block; color:#fff; line-height:20px; padding:12px 25px; text-decoration:none; vertical-align:middle; }
